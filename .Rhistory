# 计算阶乘的函数
factorial_function <- function(n) {
result <- 1
for(i in 1:n) {
result <- result * i
}
return(result)
}
# 计算10的阶乘
print(factorial_function(10))
# 从用户那里获取输入
n <- readline(prompt="请输入一个整数: ")
# 检查输入是否为正整数
if(n < 1) {
print("数字应该是正整数。")
} else {
print(paste(n, "的阶乘是:", factorial_function(n)))
}
